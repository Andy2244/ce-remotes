# table Beelink_BT, type: NEC
0x800d KEY_ENTER
0x8026 KEY_UP
0x8028 KEY_DOWN
0x8025 KEY_LEFT
0x8027 KEY_RIGHT
0x8049 KEY_MENU
0x801b KEY_BACK
0x804e KEY_VOLUMEUP
0x8056 KEY_VOLUMEDOWN
0x8041 KEY_PAGEUP
0x8042 KEY_PAGEDOWN
0x804d KEY_MUTE
0x804d KEY_DELETE
0x8051 KEY_POWER
0x8053 KEY_HOME           # HOME
0x8052 KEY_CONTEXT_MENU   # MOUSE
0x801b KEY_BACK           # BACK

